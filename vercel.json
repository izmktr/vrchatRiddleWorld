{
  "version": 2,
  "functions": {
    "api/index.py": {
      "runtime": "@vercel/python",
      "maxDuration": 30
    },
    "api/thumbnail.py": {
      "runtime": "@vercel/python",
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/world/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/api/thumbnail/(.*)",
      "dest": "/api/thumbnail.py"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    }
  ],
  "outputDirectory": "web/.next"
}